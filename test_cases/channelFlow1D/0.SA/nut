FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.25e-7;

boundaryField
{    
    bottomWall
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    
    top
    {
        type            symmetryPlane;
    }
    
    left
    {
        type            cyclic;
    }
    
    right
    {
        type            cyclic;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
    
    processor
    {
    	type            processor;
    	value           $internalField;
    }
}


// ************************************************************************* //
